{
  "name": "@{{organization}}/{{name}}",
  "version": "0.1.1",
  "scripts": {
    "docker:start": "make up",
    "docker:poststart": "docker-compose down",
    "docker:test": "make test",
    "docker:tdd": "make tdd",
    "docker:build": "make build",
    "pretest": "mkdir -p ./src/tmp",
    "test": "rm -rf ./logs && NODE_ENV=development JEST_TEST=true jest --detectOpenHandles --verbose",
    "test:dev": "JEST_TEST=true jest --watchAll",
    "test:ci": "make ci",
    "test:load": "artillery run ./scripts/load.yaml",
    "lint": "tslint --project tsconfig.json",
    "prebuild": "rm -rf ./src/schema && mkdir -p ./src/schema && npm run schema",
    "build": "tsc",
    "tsc": "npm run build",
    "run": "node dist/server.js",
    "prod": "node dist/server.js",
    "dev": "ts-node-dev ./src/server.ts",
    "prestart": "npm run prebuild",
    "start": "CI_TEST=true NODE_ENV=development ACTIVE_ENV=development ts-node-dev ./src/server.ts",
    "start:ci": "CI_TEST=true NODE_ENV=development ACTIVE_ENV=development ts-node ./src/server.ts",
    "schema": "node ./scripts/schema"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "bunyan": "^1.8.14",
    "bunyan-middleware": "^1.0.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.13.0",
    "fs": "0.0.1-security",
    "helmet": "^3.23.3",
    "node-fetch": "^2.6.0",
    "rate-limiter-flexible": "^2.1.10",
    "uuid": "^7.0.3",
    "x-xss-protection": "^1.3.0"
  },
  "devDependencies": {
    "@babel/types": "^7.11.0",
    "@types/babel__generator": "^7.6.1",
    "@types/bunyan": "^1.8.6",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.6",
    "@types/proxyquire": "^1.3.28",
    "@types/supertest": "^2.0.10",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "artillery": "^1.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-mock": "^2.3.2",
    "jest": "^26.2.2",
    "pascalcase": "^1.0.0",
    "prettier": "^2.0.5",
    "proxyquire": "^2.1.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "ts-to-json": "^0.65.4",
    "typescript": "^3.9.7",
    "yargs": "^15.4.1"
  }
}
